{
  "name": "ess-example",
  "version": "1.0.372",
  "description": "以插件 `egg-seneca-subserver` 作为主要支持的 `Egg.js` 子服务骨架",
  "private": true,
  "egg": {
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-ess-example",
    "stop": "egg-scripts stop --title=egg-server-ess-example",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "dependencies": {
    "calendar2": "^1.0.3",
    "crypto-js": "^4.0.0",
    "egg": "^2.26.0",
    "egg-cors": "^2.2.3",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.13.0",
    "egg-seneca-subserver": "^1.0.37",
    "egg-sequelize": "^5.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "mysql2": "^2.1.0",
    "seneca": "^3.20.1",
    "seneca-web": "^2.2.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.14.1",
    "egg-ci": "^1.15.0",
    "egg-mock": "^4.0.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
